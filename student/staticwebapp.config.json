{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/custom-error-pages/400.html"
    },
    "401": {
      "rewrite": "/custom-error-pages/401.html"
    },
    "403": {
      "rewrite": "/custom-error-pages/403.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "trailingSlash": "auto"
}
