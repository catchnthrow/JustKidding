{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{js,css,ico,png,jpg,gif,svg,woff,woff2}", "/chunk-*.js", "/main-*.js", "/polyfills-*.js", "/styles-*.css"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "mimeTypes": {
    ".js": "text/javascript"
  }
}